{
    "pypi": {
        "jinja2": {
            "product": "jinja",
            "repo": {
                "url": "github.com/pallets/jinja",
                "tags": "jinja2:@version",
                "includes": ["jinja2/"]
            }
        },
        "flask": {
            "repo": {
                "url": "github.com/pallets/flask",
                "src": ".",
                "includes": ["flask/", "flask.py", "src/"]
            }
        },
        "django": {
            "repo": {
                "url": "github.com/django/django",
                "includes": ["django/"]
            }
        },
        "tornado": {
            "repo": {
                "url": "github.com/tornadoweb/tornado",
                "includes": ["tornado/"],
                "excludes": ["tornado/test/"]
            }
        },
        "tensorflow": {
            "repo": {
                "url": "github.com/tensorflow/tensorflow",
                "includes": ["tensorflow/"]
            },
            "vendor": "google"
        }
    }
}