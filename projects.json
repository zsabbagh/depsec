{
    "pypi": {
        "jinja2": {
            "product": "jinja"
        },
        "flask": {
            "repo": {
                "url": "github.com/pallets/flask",
                "src": ".",
                "includes": ["flask/**/*.py", "flask.py", "src/**/*.py"]
            }
        },
        "django": {
            "repo": {
                "url": "github.com/django/django",
                "includes": ["django/**/*.py"]
            }
        },
        "tornado": {
            "repo": {
                "url": "github.com/tornadoweb/tornado",
                "includes": ["tornado/**/*.py"],
                "excludes": ["tornado/test/**"]
            }
        },
        "tensorflow": {
            "repo": {
                "url": "github.com/tensorflow/tensorflow",
                "includes": ["tensorflow/**/*.py"]
            },
            "vendor": "google"
        }
    }
}